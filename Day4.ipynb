   "source": [
    "def sum_of_even_numbers(n):\n",
    "    if n<2:\n",
    "        return 0\n",
    "    sum_even=0\n",
    "    for i in range(2,n+1,2):\n",
    "        sum_even+=i\n",
    "    return sum_even\n",
    "n=int(input(\"Enter a positive integer (n): \"))\n",
    "result=sum_of_even_numbers(n)\n",
    "print(\"The sum of all even numbers between 1 and\",n,\"is:\",result)"
   ]
  }
